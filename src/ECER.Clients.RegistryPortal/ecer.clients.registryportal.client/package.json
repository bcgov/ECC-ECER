{
  "name": "ecer.clients.registryportal.client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && npm run lint && npm run type-check && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write .",
    "gen-api": "npx openapicmd typegen http://localhost:5120/swagger/v1/swagger.json > src/types/openapi.d.ts && npm run lint-fix"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "luxon": "^3.5.0",
    "oidc-client-ts": "3.1.0",
    "openapi-client-axios": "^7.4.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "uuid": "^10.0.0",
    "vue": "^3.5.11",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.6"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^9.12.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.28.0",
    "openapicmd": "^2.6.1",
    "sass": "^1.69.5",
    "typescript": "~5.2.0",
    "vite": "^4.5.5",
    "vue-tsc": "^1.8.19"
  }
}
