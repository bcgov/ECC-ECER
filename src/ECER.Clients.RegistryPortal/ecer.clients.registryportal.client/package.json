    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "storybook": "^10.0.7",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "vite": "^7.1.12",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
