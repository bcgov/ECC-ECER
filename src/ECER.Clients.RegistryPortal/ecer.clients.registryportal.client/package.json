{
  "name": "ecer.clients.registryportal.client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && npm run lint && npm run type-check && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lintfix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "gen-api": "npx openapicmd typegen http://localhost:5120/swagger/v1/swagger.json > src/types/openapi.d.ts && npm run lintfix"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.7.7",
    "luxon": "^3.4.4",
    "oidc-client-ts": "^2.4.0",
    "openapi-client-axios": "^7.4.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "uuid": "^10.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.18.14",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "openapicmd": "^2.4.0",
    "sass": "^1.69.5",
    "typescript": "~5.2.0",
    "vite": "^4.5.5",
    "vue-tsc": "^1.8.19"
  }
}
