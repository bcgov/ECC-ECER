{
  "name": "ecer.clients.registryportal.client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && npm run lint && npm run type-check && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write .",
    "gen-api": "npx openapicmd typegen http://localhost:5120/swagger/v1/swagger.json > src/types/openapi.d.ts && npm run lint-fix"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@vueuse/core": "^12.4.0",
    "axios": "^1.7.9",
    "luxon": "^3.5.0",
    "oidc-client-ts": "3.1.0",
    "openapi-client-axios": "^7.4.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.1.3",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "openapicmd": "^2.6.1",
    "sass": "^1.82.0",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.10"
  }
}
