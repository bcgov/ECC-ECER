{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress", "cypress-file-upload"]
  },
  "include": ["cypress/**/*.ts", "cypress/types/**/*.d.ts"]
}
