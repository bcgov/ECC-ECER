    "lint-fix": "eslint . --fix",
    "format": "prettier --write .",
    "gen-api": "npx openapicmd typegen http://localhost:5140/swagger/v1/swagger.json > src/types/openapi.d.ts && npm run lint-fix",
    "cy-open": "cypress open",
    "cy-run": "cypress run",
    "e2e-chrome": "cypress run --browser chrome --headed",
